{
  "name": "react-ui-icheck",
  "description": "iCheck customizable checkboxes and radios built with React.",
  "version": "0.8.1",
  "author": {
    "name": "Victor Popkov",
    "email": "victor@popkov.me"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "is-mobile": "^3.0.0",
    "prop-types": "^15.8.1",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@types/jest": "^27.4.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "babel-eslint": "^10.1.0",
    "codecov": "^3.8.3",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "expect": "^27.4.6",
    "jest": "^27.4.7",
    "jsdom": "^19.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "raf": "^3.4.1",
    "react": "^17.0.2",
    "yarn-audit-fix": "^9.0.8",
    "yarn-deduplicate": "^3.1.0"
  },
  "engines": {
    "node": ">=12.* <13.* || >=14.*"
  },
  "esnext": "src/index.js",
  "files": [
    "lib/",
    "src/"
  ],
  "homepage": "https://github.victorpopkov.com/react-ui-icheck/",
  "keywords": [
    "checkbox",
    "form",
    "icheck",
    "input",
    "radio",
    "react",
    "react-ui-icheck",
    "switch",
    "toggle",
    "ui"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "peerDependencies": {
    "react": "^16"
  },
  "repository": {
    "type": "git",
    "url": "github:victorpopkov/react-ui-icheck"
  },
  "scripts": {
    "audit:fix": "yarn-audit-fix",
    "build": "cross-env BABEL_ENV=production NODE_ENV=production babel ./src/ -d ./lib/ --ignore './**/*.spec.jsx'",
    "ci:eslint": "eslint --config ./.eslintrc --ext js,jsx --format ./.github/eslint-issues.js .",
    "ci:prettier:md": "prettier --check './**/*.md'",
    "ci:prettier:yml": "prettier --check './**/*.yml'",
    "ci:test": "jest --coverage --coverageReporters=json-summary --json --outputFile=jest-summary.json",
    "deduplicate": "yarn-deduplicate",
    "lint": "npm-run-all --continue-on-error lint:*",
    "lint:eslint": "eslint --config ./.eslintrc --ext js,jsx .",
    "lint:prettier": "prettier --check .",
    "prepare": "npm-run-all build",
    "test": "jest --coverage",
    "test:codecov": "codecov",
    "test:watch": "jest --watch"
  }
}
