    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.1",
    "@types/jest": "^29.5.12",
    "babel-eslint": "^10.1.0",
    "caniuse-lite": "^1.0.30001634",
    "codecov": "^3.8.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
