{
  "name": "react-ui-icheck",
  "description": "iCheck customizable checkboxes and radios built with React.",
  "version": "0.6.0",
  "author": {
    "name": "Victor Popkov",
    "email": "victor@popkov.me"
  },
  "betterScripts": {
    "build:lib": {
      "command": "babel ./src/ -d ./lib/ --ignore **/*.test.js",
      "env": {
        "NODE_ENV": "production",
        "BABEL_ENV": "production"
      }
    }
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "is-mobile": "^2.1.0",
    "prop-types": "^15.7.2",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^24.0.18",
    "babel-eslint": "^10.0.3",
    "better-npm-run": "^0.1.1",
    "codecov": "^3.6.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "expect": "^24.9.0",
    "jest": "^24.9.0",
    "jsdom": "^15.1.1",
    "npm-run-all": "^4.1.5",
    "raf": "^3.4.1",
    "react": "^16.9.0",
    "remark-cli": "^7.0.0",
    "remark-preset-lint-markdown-style-guide": "^2.1.3"
  },
  "engines": {
    "node": ">= 8.10.0 < 9.* || >= 10.*"
  },
  "files": [
    "CHANGELOG.md",
    "lib",
    "README.md"
  ],
  "homepage": "http://github.victorpopkov.com/react-ui-icheck/",
  "keywords": [
    "checkbox",
    "form",
    "icheck",
    "input",
    "radio",
    "react",
    "react-ui-icheck",
    "switch",
    "toggle",
    "ui"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "peerDependencies": {
    "react": "^16"
  },
  "repository": {
    "type": "git",
    "url": "github:victorpopkov/react-ui-icheck"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:lib": "better-npm-run build:lib",
    "dev": "npm-run-all build:lib watch",
    "lint": "npm-run-all --continue-on-error lint:*",
    "lint:eslint": "eslint -c .eslintrc ./src/",
    "lint:remark": "remark .",
    "prepare": "npm-run-all build",
    "test": "jest --coverage",
    "test:codecov": "codecov",
    "test:watch": "jest --watch"
  }
}
