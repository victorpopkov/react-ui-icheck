    "webpack-merge": "^4.2.1"
  },
  "engines": {
    "node": ">= 8.10.0 < 9.* || >= 10.*"
  },
  "files": [
    "CHANGELOG.md",
    "lib",
    "README.md"
  ],
  "homepage": "http://github.victorpopkov.com/react-ui-icheck/",
  "keywords": [
    "checkbox",
    "form",
    "icheck",
    "input",
    "radio",
    "react",
    "react-ui-icheck",
    "switch",
    "toggle",
    "ui"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "peerDependencies": {
    "react": "^15 || ^16"
  },
  "repository": {
    "type": "git",
    "url": "github:victorpopkov/react-ui-icheck"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:lib": "better-npm-run build:lib",
    "build:website": "better-npm-run build:website",
    "dev": "npm-run-all build:lib watch",
    "lint": "npm-run-all --continue-on-error lint:*",
    "lint:eslint": "eslint -c .eslintrc ./src/ ./website/src/",
    "lint:remark": "remark .",
    "lint:stylelint": "stylelint './website/src/**/*.css' './website/src/**/*.scss'",
    "prepare": "npm-run-all build",
    "start": "better-npm-run start",
    "test": "jest --coverage",
    "test:codecov": "codecov",
    "test:watch": "jest --watch",
    "watch": "better-npm-run watch"
  }
}
