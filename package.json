{
  "name": "react-ui-icheck",
  "description": "iCheck customizable checkboxes and radios built with React.",
  "version": "0.4.0",
  "author": {
    "name": "Victor Popkov",
    "email": "victor@popkov.com"
  },
  "betterScripts": {
    "build:website": {
      "command": "webpack --config ./website/webpack/webpack.config.prod.js --colors --display-error-details --verbose"
    },
    "build:lib": {
      "command": "babel ./src/ -d ./lib/ --ignore **/*.test.js",
      "env": {
        "NODE_ENV": "production",
        "BABEL_ENV": "production"
      }
    },
    "start": {
      "command": "http-server website/dist/",
      "env": {
        "NODE_ENV": "production",
        "BABEL_ENV": "production"
      }
    },
    "watch": {
      "command": "webpack-dev-server --config ./website/webpack/webpack.config.dev.js",
      "env": {
        "NODE_ENV": "development",
        "BABEL_ENV": "development"
      }
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "icheck": "^1.0.2",
    "is-mobile": "^2.0.0",
    "lodash": "^4.17.11",
    "prismjs": "^1.15.0",
    "prop-types": "^15.7.2",
    "react-dom": "^16.8.2",
    "react-hot-loader": "^4.6.5",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.3.1",
    "reactstrap": "^7.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-syntax-export-default-from": "^7.2.0",
    "@babel/plugin-transform-react-display-name": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@types/jest": "^24.0.5",
    "autoprefixer": "^9.4.7",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-prismjs": "^1.0.2",
    "babel-plugin-react-css-modules": "^5.0.0",
    "babel-plugin-webpack-alias": "^2.1.2",
    "better-npm-run": "^0.1.1",
    "clean-webpack-plugin": "^1.0.1",
    "codecov": "^3.2.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.10",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.14.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-onefe": "^0.2.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "expect": "^24.1.0",
    "express": "^4.16.4",
