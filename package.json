  "license": "MIT",
  "main": "lib/index.js",
  "peerDependencies": {
    "react": "^16"
  },
  "repository": {
    "type": "git",
    "url": "github:victorpopkov/react-ui-icheck"
  },
  "scripts": {
    "audit:fix": "yarn-audit-fix",
    "build": "cross-env BABEL_ENV=production NODE_ENV=production babel ./src/ -d ./lib/ --ignore='./**/*.spec.jsx'",
